                            <TableColumn fx:id="colDescricao" prefWidth="219.0" text="Descrição" />
                              <TableColumn fx:id="colMarca" prefWidth="117.0" text="Marca" />
                              <TableColumn fx:id="colSubGrupo" prefWidth="101.0" text="SubGrupo" />
                              <TableColumn fx:id="ColPrecoVista" prefWidth="96.0" text="Preço Vista" />
