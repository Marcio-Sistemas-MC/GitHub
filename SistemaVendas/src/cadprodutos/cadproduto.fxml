            <Button fx:id="btnAnterior" mnemonicParsing="false" onAction="#actionBtnAnterior" prefHeight="25.0" prefWidth="35.0" text="&lt;" />
            <Button fx:id="btnProximo" mnemonicParsing="false" onAction="#actionBtnProximo" prefHeight="25.0" prefWidth="35.0" text="&gt;" />
            <Button fx:id="btnUltimo" mnemonicParsing="false" onAction="#actionBtnUltimo" prefHeight="25.0" prefWidth="38.0" text="&gt;&gt;" />
          <Button fx:id="btnNovo" mnemonicParsing="false" onAction="#actionBtnNovo" prefHeight="25.0" prefWidth="51.0" text="Novo" />
            <Button fx:id="btnEditar" mnemonicParsing="false" onAction="#actionBtnEditar" prefHeight="25.0" prefWidth="55.0" text="Editar" />
            <Button fx:id="btnSalvar" mnemonicParsing="false" onAction="#actionBtnSalvar" prefHeight="25.0" prefWidth="53.0" text="Salvar" />
            <Button fx:id="btnExcluir" mnemonicParsing="false" onAction="#actionBtnExcluir" prefHeight="25.0" prefWidth="57.0" text="Excluir" />
            <Button fx:id="btnSair" mnemonicParsing="false" onAction="#actionBtnSair" prefHeight="25.0" prefWidth="55.0" text="Sair" />
        </items>
      </ToolBar>
   </children>
</AnchorPane>
