<AnchorPane id="AnchorPane" prefHeight="592.0" prefWidth="700.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cadprodutos.CadProdutoController">
